				float NDotV34 = dotResult33;
				float2 temp_cast_3 = (NDotV34).xx;
				float2 lerpResult26 = lerp( appendResult23 , temp_cast_3 , _refractStrength);
				float4 fetchOpaqueVal27 = float4( SHADERGRAPH_SAMPLE_SCENE_COLOR( lerpResult26 ), 1.0 );
				float4 appendResult93 = (float4(( ase_grabScreenPosNorm.r + _ChromaticOffset ) , ase_grabScreenPosNorm.g , 0.0 , 0.0));
				float4 fetchOpaqueVal78 = float4( SHADERGRAPH_SAMPLE_SCENE_COLOR( appendResult93.xy ), 1.0 );
				float4 appendResult94 = (float4(( ase_grabScreenPosNorm.r + ( _ChromaticOffset * 0.5 ) ) , ase_grabScreenPosNorm.g , 0.0 , 0.0));
				float4 fetchOpaqueVal79 = float4( SHADERGRAPH_SAMPLE_SCENE_COLOR( appendResult94.xy ), 1.0 );
				float4 appendResult95 = (float4(( ase_grabScreenPosNorm.r + ( _ChromaticOffset * -0.5 ) ) , ase_grabScreenPosNorm.g , 0.0 , 0.0));
				float4 fetchOpaqueVal80 = float4( SHADERGRAPH_SAMPLE_SCENE_COLOR( appendResult95.xy ), 1.0 );
				float4 appendResult97 = (float4(fetchOpaqueVal78.r , fetchOpaqueVal79.g , fetchOpaqueVal80.b , 0.0));
				float smoothstepResult42 = smoothstep( _fresnelMin , _fresnelMax , NDotV34);
				float Fresnel46 = ( ( 1.0 - smoothstepResult42 ) * _fresnelVal );
				float4 lerpResult99 = lerp( fetchOpaqueVal27 , appendResult97 , Fresnel46);
				float4 refract28 = lerpResult99;
				float2 uv_Texture = IN.ase_texcoord5.xy * _Texture_ST.xy + _Texture_ST.zw;
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
